    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "rollup": "^3.25.3",
    "rollup-plugin-multi-input": "^1.4.1",
    "simple-mock": "^0.8.0",
    "superagent": "^6.1.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
